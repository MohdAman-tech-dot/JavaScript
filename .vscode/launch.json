    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        },
        {
            "type": "chrome",
            "name": "http://127.0.0.1:3000/14-Id & Classes.html",
            "request": "launch",
            "url": "http://127.0.0.1:3000/14-Id & Classes.html"
        },
        {
            "type": "chrome",
            "name": "http://127.0.0.1:3000/7-PracticeSet_3.html",
            "request": "launch",
            "url": "http://127.0.0.1:3000/7-PracticeSet_3.html"
        }
    ]
}